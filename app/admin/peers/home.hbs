<div class="admin">
  <div class="heading">
    <h2 id="afiliacionesTitle">Ãšltimas afiliaciones {{#if peers}}({{peers.length}}){{/if}}</h2>
  </div>
    {{#if groups}}
      {{#each groups}}
        <div class="groupHeader">
          {{dateString}}
        </div>
        {{#if peers}}
          <div class="peersList boxes">
            {{#each peers}}
                <div class="peerContainer visible" data-id="{{id}}" data-field-nombre="{{nombreSlug}}" data-field-email="{{emailSlug}}">
                    <div class="peerNombre">
                    {{apellido}}, {{nombre}}
                  </div><div class="peerEmail">
                    {{email}}
                  </div><div class="peerTelefono">
                    {{telefono}}
                  </div><div class="actionsContainer">
                    <a class="button delete" href="#">
                    </a><a class="button edit" href="/admin/peers/{{id}}/edit">
                    </a><a class="button mail" href="mailto:{{email}}"></a>
                  </div>
                </div>
            {{/each}}
          </div>
        {{/if}}
      {{/each}}
    {{else}}
      <div class="noItems">No hay grupos</div>
    {{/if}}
</div>